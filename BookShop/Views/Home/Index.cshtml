    ViewData["Title"] = "Home";
}
@*<vc:go></vc:go>*@ @*just for test*@
<div class="jumbotron justify-content-md-center bg-white">
    <div class="container">
        <h1 class="text-center font-weight-bolder">Welcom to Book Store</h1>
        <br />
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" >
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/Images/20191219170611-GettyImages-1152794789.jpeg" alt="First slide" width="360" height="600">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Images/98b784d2d48fcdf0125d381510ea5360.jpg" alt="Second slide"width="360" height="600">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Images/How-to-format-a-book.jpg" alt="Third slide"width="360" height="600">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/Images/book-dark-enlightenment-1029141-1024x683.jpg" alt="forth slide"width="360" height="600">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
